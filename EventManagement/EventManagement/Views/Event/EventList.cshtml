@model List<EventList>

@{
    ViewData["Title"] = "Event";
}

<div class="card">
    <div class="card-body">
        <h5 class="card-title">@EventList.Caption</h5>
        <p class="card-text">@EventList.Description</p>
        <ul class="list-group list-group-flush">
            <li class="list-group-item"><strong>Date:</strong> @EventList.Date</li>
            <li class="list-group-item"><strong>Participants:</strong> @EventList.Participants</li>
        </ul>
    </div>
    <div class="card-footer text-right">
        <a href="#" class="btn btn-primary">Detay</a>
    </div>
</div>

</table>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)

